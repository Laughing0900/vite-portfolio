{
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 4,
    "arrowParens": "always",
    "semi": true,
    "singleQuote": false,
    "useTabs": false,
    "plugins": [
        "@ianvs/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
        "<TYPES>",
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "^@/*/(.*)$",
        "^[../]",
        "^[./]",
        "<TYPES>"
    ]
}